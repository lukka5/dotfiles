[user]
    name = Lucas Godoy
    email = 2lucasg@gmail.com

[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    s = stash
    ch = cherry -v
    last = log -1 -p HEAD
    unstage = reset HEAD --
    visual = !gitg
    conflicts = diff --name-only --diff-filter=U
    who = shortlog -s --
    hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)[%an]%Creset' --abbrev-commit --date=relative
    ls = log --graph --pretty=format:'%Cred%h %Cgreen%ad%Creset -%C(yellow)%d %Creset%s%C(bold blue) [%cn]%Creset' --decorate --date=short
    ll = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)[%an]%Creset' --decorate --numstat
    la = "!git config -l | grep alias | cut -c 7-" # list aliases
    lss = log --graph --pretty=format:'%Cred%h %Cgreen%ad%Creset -%C(yellow)%d %Creset%<(35,trunc)%s%C(bold blue) [%cn]%Creset' --decorate --date=short
    hists = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %<(30,trunc)%s %Cgreen(%cr) %C(bold blue)[%an]%Creset' --abbrev-commit --date=relative

[core]
    editor = vim
    excludesfile = /Users/lucas/.gitignore
    quotepath = false

[color]
    ui = true

[credential]
    helper = cache --timeout=7200

[merge]
    tool = vimdiff
[push]
	default = matching
[url "https://"]
	insteadOf = git://
